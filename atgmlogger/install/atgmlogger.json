{
  "version": 0.4,
  "sensor": {
    "name": "AT1M-10"
  },
  "serial": {
    "port": "/dev/serial0",
    "baudrate": 57600,
    "bytesize": 8,
    "parity": "N",
    "stopbits": 1
  },
  "logging": {
    "logdir": "/var/log/atgmlogger"
  },
  "plugins": {
    "gpio": {
        "mode": "board",
        "data_pin": 11,
        "usb_pin": 13,
        "freq": 0.04
    },
    "usb": {
        "mountpath": "/media/removable",
        "logdir": "/var/log/atgmlogger",
        "patterns": ["*.dat", "*.dat.*", "*.log", "*.gz"]
    },
    "timesync": {
        "interval": 1000
    }
  }
}
